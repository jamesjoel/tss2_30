<%- include('header') %>

<div class="container">
    <div class="row">
        <div class="col-md-10 offset-md-1">
        </br>
    </br>
</br>
<h1 class="text-center m-0 p-0">Contact For Any Inquiries</h1>
<p class=" text-center m-0">click request button</p>
<div class="container text-center mb-5">
<a href="/contact/request" class="btn btn-info">Request</a>
</div>
<div class="card">
    <div class="card-header text-center">
        <h3><b>Request List</b></h3>
    </div>
    <div class="card-body p-0">
        <table class="table table-dark table-hover table-bordered text-center ">
            <tr>
                <th>S.no</th>
                <th>Name</th>
                <th>City</th>
                <th>Contact</th>
                <th>More info</th>
            </tr>
            <% result.forEach((x, n)=>{ %>
            
            <tr>
                <td><%= n+1 %></td>
                <td><%= x.f_name %></td>
                <td><%= x.city %></td>
                <td><%= x.contact %></td>
                <td><a href="/contact/view/<%= x._id %>" class="btn btn-sm btn-info">view</a></td>
            </tr>
            
            
            
            
            <% }) %>
            
        </table>
    </div>
</div>
</div>
</div>
</div>


<%- include('footer') %>