<%- include('header') %>
<div class="container" style="min-height:650px">
    <h3>view page</h3>
    <div class="row">
        <div class="col-md-8 offset-md-2">
            <div class="card">
                <div class="card-header">
                    <h3>Student Information</h3>
                </div>
                <div class="card-body p-0">
                    <table class="table table-hover table-bordered table-dark ">
                        <tr>
                            <td>Name</td>
                            <td><%= result.s_name%></td>
                        </tr>
                        <tr>
                            <td>Class</td>
                            <td><%= result.s_edu%></td>
                        </tr>
                        <tr>
                            <td>Subject</td>
                            <td><%= result.s_sub%></td>
                        </tr>
                        <tr>
                            <td>Cast</td>
                            <td><%= result.cast%></td>
                        </tr>
                        <tr>
                            <td>Fees</td>
                            <td><%= result.fees%></td>
                        </tr>
                        

                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<%- include('footer') %>